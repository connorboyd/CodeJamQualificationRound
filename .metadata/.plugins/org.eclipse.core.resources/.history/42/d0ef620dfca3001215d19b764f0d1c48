import java.io.*;
import java.util.*;

public class Main {

	
	private static String inputFileName = "input.txt";
	private static int sampleSize;
	
	public static void main(String[] args) throws IOException
	{
		getStuffFromFile();
	}
	
	public static void getStuffFromFile() throws IOException
	{
		Scanner myScanner = new Scanner(new FileReader(inputFileName));
		sampleSize = myScanner.nextInt();
		
	}
	
	
}
